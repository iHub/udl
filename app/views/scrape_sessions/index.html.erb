<% provide(:page_title, "All Sessions") %>
<% provide(:app_page, "scrape_sessions" ) %>
<% provide(:app_sub, "scrape_sessions_index" ) %>

<div class="page-header">
	<h2 class="pull-left">Sessions</h2>
	<!-- <ul class="nav nav-pills pull-right">
		<li><%= link_to "Logs", "#" %></li>
	</ul> -->
	<div class="clearfix"></div>
</div>

<% if @scrape_sessions.any? %>

	<div class="panel panel-default dash-stats-sessions">
	  <div class="panel-heading sessions">All Sessions</div>
	  <div class="panel-body">
	   <%= render 'shared/scrape_session_list', object: @scrape_sessions %>
	  </div>
	  <div class="panel-footer"><%= will_paginate @scrape_sessions %></div>
	</div>
	

<% else %>
	<div class="well well-sm">
		<h4>You don't have any sessions</h4>
	</div>
<% end %>

<% content_for :debug_dump do %>

<% end %>