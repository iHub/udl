<% provide(:page_title, "New Session") %>
<% provide(:app_page, "scrape_sessions" ) %>
<% provide(:app_sub, "scrape_sessions_new" ) %>

<div class="page-header">
	<h2>Create a Session</h2>
</div>

<div class="well well-sm col-md-8 default-instructions">
    <h4>The following default values will be used if you don't set them.</h4>
    <ul>
        <li>Scrape Frequency : <span>10 Minutes</span></li> 
        <li>Continous Scrape : <span>False</span></li>
        <li>Allow Page override : <span>False</span></li>
    </ul>
</div>

<div class="col-md-8 panel panel-default">

	<div class="panel-body">
		<%= render 'shared/scrape_session_form', object: @scrape_session %>
	</div>
</div>
 