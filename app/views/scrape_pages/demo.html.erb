<div class="page-header">
	<h2>Index for Scrap Pages</h2>
</div>

<div class="page-header">
	<h3>Root Keys and values</h3>
</div>
<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>#</th>
        <th>Key</th>
        <th>value</th>       
      </tr>
    </thead>

    <tbody>
      <% counter = 0 %>
      <% @page_feed.first.each do |key, value| %>
        <% counter +=1 %>
        <tr>
          <td><%= counter %></td>
          <td><%= key %></td>
          <td><%= value %></td>          
        </tr>
      <% end %>
      
    </tbody>
  </table>
</div>

<div class="container">
	<ul class="list-group">
		<% @page_feed.each do |page_object_item| %>
			<% page_object_item.each do |object_content| %>
				<% object_content.each do |key, value| %>
					<li class="list-group-item"><%= key %></li>
				<% end %>
			<% end %>
		<% end %>
	</ul>
</div>


<% content_for :debug_dump do %>
	<h4>Access Token</h4>
  	<p><%= @stuff %></p>
	<br />

	
<% end %>
